  "eslint.alwaysShowStatus": true,
  "eslint.options": {
    "extensions": [
      ".html",
      ".js",
      ".vue",
      ".jsx"
    ]
  },
  "eslint.run": "onSave",
  "eslint.validate": [
    {
      "language": "html",
      "autoFix": true
    },
    {
      "language": "vue",
      "autoFix": true
    },
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "javascriptreact",
      "autoFix": true
    }
  ],
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 200,
  "emmet.includeLanguages": {
    "vue-html": "html",
    "vue": "sass"
  },
  "explorer.confirmDelete": false,
  "files.autoSave": "off",
  "files.autoSaveDelay": 200,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "gitlens.currentLine.scrollable": false,
  "haskell.hlint.executablePath": "hlint",
  "javascript.implicitProjectConfig.experimentalDecorators": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "search.exclude": {
    "**/_gsdata_": true,
    "**/.next": true
  },
  "search.runInExtensionHost": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.shell.windows": "",
  "vim.autoindent": true,
  "vim.easymotion": true,
  "vim.easymotionMarkerFontFamily": "'Cascadia Code PL'",
  "vim.easymotionMarkerForegroundColorOneChar": "#fff",
  "vim.easymotionMarkerForegroundColorTwoChar": "#fff",
  "vim.easymotionMarkerHeight": 24,
  "vim.easymotionMarkerWidthPerChar": 10,
  "vim.easymotionMarkerYOffset": 4,
  "vim.foldfix": true,
  "vim.history": 200,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "f"
      ],
      "commands": [
        {
          "command": "workbench.action.quickOpen"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "b"
      ],
      "commands": [
        "bookmarks.listFromAllFiles"
      ]
    },
    {
      "before": [
        "<leader>",
        "m"
      ],
      "commands": [
        "bookmarks.toggle"
      ]
    },
    {
      "before": [
        "<leader>",
        "r"
      ],
      "commands": [
        "code-runner.run"
      ]
    },
    {
      "before": [
        "<leader>",
        "z"
      ],
      "commands": [
        "workbench.action.toggleZenMode"
      ]
    },
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "p"
      ],
      "after": [
        "p",
        "g",
        "v",
        "y"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    },
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    }
  ],
  "vim.useSystemClipboard": true,
  "vim.visualstar": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.letterSpacing": 1,
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.editor.tabSizing": "shrink",
  "workbench.startupEditor": "none",
  "workbench.statusBar.feedback.visible": false,
  "terminal.integrated.fontSize": 14,
  "explorer.confirmDragAndDrop": false,
  "git.enableSmartCommit": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "html.format.indentHandlebars": true,
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "prettier.semi": false,
  "editor.formatOnSave": true,
  "eslint.packageManager": "pnpm",
  "editor.tabSize": 2,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "git.confirmSync": false,
  "editor.lineHeight": 24,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.sortOrder": "type",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
